<h1>Politicians#index</h1>
<p>Find me in app/views/politicians/index.html.erb</p>

<h2>Politicians</h2>

<ul>
  <% @politicians.each_with_index do |politician, index| %>
    <li> <%= index %>. <%= politician.first_name %> - <%= politician.last_name %></li>
  <% end %>
</ul>

<h2>Locations</h2>

<ul>
  <% @locations.each do |location| %>
    <li><%= location.name %></li>
  <% end %>
</ul>

